<?xml version="1.0" encoding="utf-8"?><!--
   Backup rules for devices older than API Level 31 (Android 12)
   See https://developer.android.com/guide/topics/data/autobackup
   for details.
-->
<full-backup-content>
    <!-- Include all shared preferences by default -->
    <include domain="sharedpref" path="."/>
    
    <!-- Exclude onboarding state - new users should always see onboarding -->
    <exclude domain="sharedpref" path="OnboardingState.xml"/>
</full-backup-content> 